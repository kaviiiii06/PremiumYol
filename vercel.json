{
  "version": 2,
  "builds": [
    {
      "src": "TrendyolClone/TrendyolClone.csproj",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "TrendyolClone/bin/Release/net8.0/publish"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index"
    }
  ],
  "env": {
    "ASPNETCORE_ENVIRONMENT": "Production"
  }
}
